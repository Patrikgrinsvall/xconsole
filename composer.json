{
    "name": "patrikgrinsvall/xconsole",
    "description": "Laravel xconsole is a local development web server in pure php with file watching, powerfull cache cleaning and more..",
    "keywords": [
        "console",
        "command-line",
        "php",
        "cli",
        "laravel",
        "artisan"
    ],
    "license": "MIT",
    "support": {
        "source": "https://github.com/patrikgrinsvall/xconsole"
    },
    "authors": [
        {
            "name": "Patrik Grinsvall",
            "email": "patrik@silentridge.io"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/console": "^9",
        "illuminate/support": "^9",
        "symfony/console": "^6.0"

    },
    "require-dev": {
        "phpunit/phpunit": "^8.5.8|^9.0"
    },
    "autoload": {
        "psr-4": {
            "PatrikGrinsvall\\XConsole\\": "src"
        },
        "psr-0": {
            "xconsole\\helpers":"src/helpers/xconsole-helpers.php"
        }
    },
    "version": "dev-master",
    "scripts": {
        "publish": "@php artisan vendor:publish --provider PatrikGrinsvall\\XConsole\\TrivialTheme\\TrivialThemeServiceProvider",
        "pr": "echo 'creating pr using default ssh-provider'",
        "tag": "echo tagging a new release",
        "post-root-package-install": [
            "@composer run publish"
        ],
        "post-package-update": [
            "@composer run publish"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "PatrikGrinsvall\\XConsole\\ServiceProviders\\XConsoleServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
